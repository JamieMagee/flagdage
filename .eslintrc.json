{
  "extends": ["xo", "prettier"],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 90
      }
    ],
    "func-names": ["error", "as-needed"],
    "camelcase": "off"
  },
  "plugins": ["prettier"]
}
